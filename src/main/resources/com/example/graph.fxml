<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.GraphController">
    <children>
        <PieChart fx:id="pieChart" layoutX="20.0" layoutY="20.0" prefHeight="400.0" prefWidth="450.0" />
        <LineChart fx:id="lineChart" layoutX="500.0" layoutY="20.0" prefHeight="400.0" prefWidth="600.0">
            <xAxis>
                <CategoryAxis fx:id="xAxis" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis" />
            </yAxis>
        </LineChart>
        <Button fx:id="backButton" text="戻る" layoutX="20.0" layoutY="450.0" prefWidth="100.0" />
    </children>
</AnchorPane>
