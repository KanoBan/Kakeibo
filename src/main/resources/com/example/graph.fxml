<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.GraphController">
    <children>
        <VBox spacing="20.0" layoutX="20.0" layoutY="20.0" prefWidth="980.0" prefHeight="600.0" style="-fx-padding: 20;">
            <children>
                <PieChart fx:id="pieChart" prefHeight="300.0" prefWidth="400.0" />
                <LineChart fx:id="lineChart" prefHeight="300.0" prefWidth="940.0">
                    <xAxis>
                        <CategoryAxis fx:id="xAxis" />
                    </xAxis>
                    <yAxis>
                        <NumberAxis fx:id="yAxis" />
                    </yAxis>
                </LineChart>
                <Button fx:id="backButton" text="戻る" prefWidth="100.0" style="-fx-margin-top: 10;" />
            </children>
        </VBox>
    </children>
</AnchorPane>
