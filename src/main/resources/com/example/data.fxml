<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.DataController">
    <children>
        <TableView fx:id="dataTable" layoutX="20.0" layoutY="20.0" prefHeight="550.0" prefWidth="750.0">
            <columns>
                <TableColumn fx:id="dateColumn" text="日付" prefWidth="150.0" />
                <TableColumn fx:id="categoryColumn" text="カテゴリー" prefWidth="300.0" />
                <TableColumn fx:id="amountColumn" text="金額" prefWidth="200.0" />
                <TableColumn fx:id="descriptionColumn" text="説明" prefWidth="300.0" />
            </columns>
        </TableView>
        <Button fx:id="backButton" text="戻る" layoutX="20.0" layoutY="590.0" />
    </children>
</AnchorPane>
